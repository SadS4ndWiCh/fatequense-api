{
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "^(.*)/dayjs.config$",
    "<THIRD_PARTY_MODULES>",
    "^(.*)/core/(.*)$",
    "^(.*)/controllers/(.*)$",
    "^(.*)/routes/(.*)$",
    "^(.*)/hooks/(.*)$",
    "^(.*)/libs/(.*)$",
    "^(.*)/errors/(.*)$",
    "^(.*)/utils/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
